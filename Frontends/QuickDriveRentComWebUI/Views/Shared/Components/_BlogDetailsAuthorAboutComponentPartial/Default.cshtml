@model List<QuickDriveRentCom.Dto.AuthorDtos.ResultGetAuthorByBlogAuthorIdDto>
<div class="about-author d-flex p-4 bg-light">
    @foreach(var item in Model)
    {
          <div class="bio mr-5">
        <img src="@item.AuthorImgUrl" alt="Image" class="img-fluid mb-4">
    </div>
    <div class="desc">
        <h3>@item.AuthorName</h3>
            <p>@item.AuthorDesc</p>
    </div>
    }
  
</div>
